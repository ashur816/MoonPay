<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.martin.dao.VoucherMapper">
    <sql id="baseSql">
        voucher_id,policy_id,user_id,face_value,effect_time,expire_time,receive_time,use_time,state
    </sql>
    <select id="selectVoucherByUser" resultType="com.martin.bean.VoucherBean">
        select
        <include refid="baseSql"/>
        from m_voucher WHERE user_id=#{userId} and state=#{state};
    </select>
</mapper>