<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean id="payParam" class="com.martin.constant.PayParam">
        <property name="webBody" value="${pay.webBody}"></property>
        <property name="appBody" value="${pay.appBody}"></property>
        <property name="certPath" value="${pay.certPath}"></property>
        <property name="homeUrl" value="${home.url}"></property>

        <property name="aliAuthRetUrl" value="${alipay.authRetUrl}"></property>
        <property name="aliReturnUrl" value="${alipay.returnUrl}"></property>
        <property name="aliRefundNotifyUrl" value="${alipay.refundNotifyUrl}"></property>
        <property name="aliTransferNotifyUrl" value="${alipay.transferNotifyUrl}"></property>
        <property name="aliWebNotifyUrl" value="${alipay.webNotifyUrl}"></property>
        <property name="aliAppNotifyUrl" value="${alipay.appNotifyUrl}"></property>
        <property name="aliPartnerId" value="${alipay.partnerId}"></property>
        <property name="aliAccountNo" value="${alipay.accountNo}"></property>
        <property name="aliAccountName" value="${alipay.accountName}"></property>
        <property name="aliAliPublicKey" value="${alipay.aliPublicKey}"></property>
        <property name="aliMD5Key" value="${alipay.MD5Key}"></property>

        <property name="aliWebAppId" value="${alipay.webAppId}"></property>
        <property name="aliWebPrivateKey" value="${alipay.webPrivateKey}"></property>

        <property name="aliAppAppId" value="${alipay.appAppId}"></property>
        <property name="aliAppPrivateKey" value="${alipay.appPrivateKey}"></property>

        <property name="tenWebNotifyUrl" value="${tenpay.webNotifyUrl}"></property>
        <property name="tenAppNotifyUrl" value="${tenpay.appNotifyUrl}"></property>

        <property name="tenWebAppId" value="${tenpay.webAppid}"></property>
        <property name="tenWebMchId" value="${tenpay.webMchId}"></property>
        <property name="tenDeviceInfo" value="${tenpay.webDeviceInfo}"></property>
        <property name="tenWebPrivateKey" value="${tenpay.webPrivateKey}"></property>
        <property name="tenAppSecret" value="${tenpay.webAppSecret}"></property>
        <property name="tenWebAuthRetUrl" value="${tenpay.webAuthRetUrl}"></property>
        <property name="tenWebReturnUrl" value="${tenpay.webReturnUrl}"></property>

        <property name="tenAppAppId" value="${tenpay.appAppid}"></property>
        <property name="tenAppMchId" value="${tenpay.appMchId}"></property>
        <property name="tenAppPrivateKey" value="${tenpay.appPrivateKey}"></property>
    </bean>
</beans>
